<fsmddescription>

  <statelist>
    <state>START</state>
    <state>VALIDATE</state>
    <state>TEST</state>
    <state>AMODB</state>
    <state>BMODA</state>
    <state>COMPLETE</state>
  </statelist>

  <initialstate>START</initialstate>

  <inputlist>
    <input>in_A</input>
    <input>in_B</input>
  </inputlist>

  <variablelist>
    <variable>var_A</variable>
    <variable>var_B</variable>
  </variablelist>

  <operationlist>
    <operation>
      <name>init_A</name>
      <expression>var_A = in_A</expression>
    </operation>
    <operation>
      <name>init_B</name>
      <expression>var_B = in_B</expression>
    </operation>
    <operation>
      <name>DECR_A</name>
      <expression>var_A = var_A - 1</expression>
    </operation>
    <operation>
      <name>DECR_B</name>
      <expression>var_B = var_B - 1</expression>
    </operation>
    </operationlist>

  <conditionlist>
    <condition>
      <name>A_equal_B</name>
      <expression>var_A == var_B</expression>
    </condition>
    <condition>
      <name>A_greater_B</name>
      <expression>var_A &gt; var_B</expression>
    </condition>
    <condition>
      <name>B_greater_A</name>
      <expression>var_A &lt; var_B</expression>
    </condition>
    <condition>
      <name>A_MOD_B_ZERO</name>
      <expression>(var_A % var_B) == 0</expression>
    </condition>
    <condition>
      <name>A_MOD_B_NZERO</name>
      <expression>(var_A % var_B) != 0</expression>
    </condition>
    <condition>
      <name>B_MOD_A_ZERO</name>
      <expression>(var_B % var_A) == 0</expression>
    </condition>
    <condition>
      <name>B_MOD_A_NZERO</name>
      <expression>(var_B % var_A) != 0</expression>
    </condition>




  </conditionlist>

  <fsmd>
    <START>
      <transition>
        <condition>True</condition>
        <instruction>init_A init_B</instruction>
        <nextstate>TEST</nextstate>
      </transition>
    </START>

    <TEST>
      <transition>
        <condition>A_equal_B</condition>
        <instruction>NOP</instruction>
        <nextstate>COMPLETE</nextstate>
      </transition>
      <transition>
        <condition>A_greater_B</condition>
        <instruction>NOP</instruction>
        <nextstate>AMODB</nextstate>
      </transition>
      <transition>
        <condition>B_greater_A</condition>
        <instruction>NOP</instruction>
        <nextstate>BMODA</nextstate>
      </transition>
    </TEST>

    <AMODB>
      <transition>
        <condition>A_MOD_B_NZERO</condition>
        <instruction>DECR_B</instruction>
        <nextstate>TEST</nextstate>
      </transition>
      <transition>
        <condition>A_MOD_B_ZERO</condition>
        <instruction>NOP</instruction>
        <nextstate>COMPLETE</nextstate>
      </transition>
    </AMODB>

    <BMODA>
      <transition>
        <condition>B_MOD_A_NZERO</condition>
        <instruction>DECR_A</instruction>
        <nextstate>TEST</nextstate>
      </transition>
      <transition>
        <condition>B_MOD_A_ZERO</condition>
        <instruction>NOP</instruction>
        <nextstate>COMPLETE</nextstate>
      </transition>
    </BMODA>

    <COMPLETE>
      <transition>
        <condition>True</condition>
        <instruction>NOP</instruction>
        <nextstate>COMPLETE</nextstate>
      </transition>
    </COMPLETE>
  </fsmd>

</fsmddescription>
